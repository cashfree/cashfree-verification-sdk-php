{
    "name": "cashfree/cashfree-verification",
    "version": "3.0.0",
    "description": "Cashfree's Verification APIs provide different types of verification to our merchants.",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "api",
        "cashfree",
        "verification suite"
    ],
    "homepage": "https://www.cashfree.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Cashfree Payments",
            "homepage": "https://www.cashfree.com"
        }
    ],
    "support": {
        "email": "care@cashfree.com",
        "issues": "https://github.com/cashfree/cashfree-verification-sdk-php/issues",
        "source": "https://github.com/cashfree/cashfree-verification-sdk-php"
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "sentry/sdk": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Cashfree\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Cashfree\\Test\\" : "test/" }
    }
}
